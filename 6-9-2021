    @Override
    public void onEnable() {
        this.getCommand("start").setExecutor(new start());
        getServer().getPluginManager().registerEvents(this,this);

        int e=0;
        colorMap.put("RED",colors[0]);
        colorMap.put("BLUE",colors[1]);
